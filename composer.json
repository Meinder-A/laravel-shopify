{
    "name": "meinder-a/laravel-shopify",
    "description": "Shopify package for Laravel to aide in app development",
    "homepage": "https://github.com/meinder-a/laravel-shopify",
    "license": "MIT",
    "keywords": [
        "php",
        "shopify",
        "laravel"
    ],
    "authors": [
        {
            "name": "Anthony Meinder",
            "email": "me@meinder-a.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MeinderA\\LaravelShopify\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MeinderA\\LaravelShopify\\LaravelShopifyServiceProvider"
            ]
        }
    }
}
